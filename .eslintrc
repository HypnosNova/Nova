{
    "extends": "mdcs",
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaVersion": 9,
        "sourceType": "module",
        "ecmaFeatures": {
            "arrowFunctions": true,
            "jsx": true,
            "js": true
        }
    },
    "plugins": [
        "babel"
    ]
}